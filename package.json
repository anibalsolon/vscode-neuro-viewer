{
	"name": "neuro-viewer",
	"displayName": "NeuroViewer",
	"description": "View your neuroimaging files in VSCode!",
	"version": "0.0.6",
	"publisher": "anibalsolon",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Visualization",
		"Data Science",
		"Other"
	],
	"keywords": [
		"neuroimaging",
		"neuroscience",
		"MRI",
		"Nifti"
	],
	"icon": "neuro-viewer.png",
	"galleryBanner": {
		"color": "#1e1e1e",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/anibalsolon/vscode-neuro-viewer.git"
	},
	"bugs": {
		"url": "https://github.com/anibalsolon/vscode-neuro-viewer/issues",
		"email": "anibalsolon@gmail.com"
	},
	"activationEvents": [
		"onCustomEditor:neuro-viewer.Nifti"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "neuro-viewer.Nifti",
				"displayName": "Nifti Viewer",
				"selector": [
					{
						"filenamePattern": "*.nii"
					},
					{
						"filenamePattern": "*.nii.gz"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.19.7",
		"@types/uuid": "^8.3.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"copy-webpack-plugin": "^6.3.2",
		"eslint": "^7.1.0",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"webpack": "^5.8.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"nifti-reader-js": "^0.5.4",
		"uuid": "^8.3.1"
	}
}
